{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      "warn",
      2
    ],
    "semi": 1,
    "no-console": 1,
    "camelcase": 1,
    "no-unused-vars": 1,
    "no-unused-expressions": 2,
    "no-var": 1
  }
}
